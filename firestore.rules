rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /users/{userId} {
      allow read, write: if true;
    }
    match /cities/{doc} {
      allow read, write: if true;
    }
    match /citySubmissions/{userId} {
      allow read, write: if true;
    }
    match /removed/{doc} {
      allow read, write: if true;
    }
    match /spins/{doc} {
      allow read, write: if true;
    }
    match /config/{doc} {
      allow read, write: if true;
    }
  }
}
